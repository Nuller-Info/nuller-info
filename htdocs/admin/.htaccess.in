DirectoryIndex index.shtml
Options +Includes +FollowSymLinks
SetEnv HAVE_WEASYPRINT @HAVE_WEASYPRINT@
SetEnv SYNC_SOURCE @SYNC_SOURCE@
SetEnv SYNC_TARGET @SYNC_TARGET@
@SSI_LEGACY_EXPR_PARSER@

RewriteEngine On
RewriteBase /admin/

# Don't rewrite files
##RewriteCond %{REQUEST_FILENAME} -f [OR]
##RewriteCond %{REQUEST_FILENAME} -d
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

# Rewrite everything else to index.shtml to allow html5 state links
RewriteRule ^ index.shtml [L]
